        },
        "user_id": {
          "type": "number"
        },
        "workflowUuid": {
          "type": "string",
          "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$"
        }
      }
    },
    {
      "required": [
        "custom_name",
        "nodeRelationshipUuid",
