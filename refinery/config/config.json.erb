{
  "ACCOUNT_ACTIVATION_DAYS": 7,
  "ADMINS": [
    ["Refinery Admin", "<%= @admin || "admin@example.org" %>"]
  ],
  "ALLOWED_HOSTS": ["localhost", "127.0.0.1", "192.168.50.50"
    <%= @site_url ? ', "' + @site_url +  '"' : "" %>
    ],
  "AUTH_LDAP_BASE_DN": "OU=Domain Users,DC=Domain",
  "AUTH_LDAP_BIND_DN": "",
  "AUTH_LDAP_BIND_PASSWORD": "",
  "AUTH_LDAP_FILTERSTR": "(uid=%(user)s)",
  "AUTH_LDAP_SCOPE": 2,
  "AUTH_LDAP_SERVER_URI": "ldap://auth.example.org",
  "AUTH_LDAP_USER_ATTR_MAP": {
    "email": "mail",
    "first_name": "givenName",
    "last_name": "sn"
  },
  "BROKER_URL": "amqp://guest:guest@localhost:5672//",
  "CELERYD_MAX_TASKS_PER_CHILD": 100,
  "DATABASES": {
    "default": {
        "ENGINE": "django.db.backends.postgresql_psycopg2",
        "HOST": "<%= @rds_host %>",
        "NAME": "refinery",
        "PASSWORD": "<%= @rds_role_password %>",
        "PORT": "<%= @rds_port %>",
        "USER": "<%= @rds_role || "vagrant" %>"
    }
  },
  "DEFAULT_FROM_EMAIL": "<%= @default_from_email || "webmaster@localhost" %>",
  "EMAIL_HOST": "<%= @email_host || "localhost" %>",
  "EMAIL_PORT": 25,
  "EMAIL_HOST_USER": "<%= @email_host_user %>",
  "EMAIL_HOST_PASSWORD": "<%= @email_host_password %>",
  "EMAIL_USE_TLS": <%= @email_use_tls || "false" %>,
  "EMAIL_SUBJECT_PREFIX": "[Refinery] ",
  "INTERVAL_BETWEEN_CHECKS": {
    "CELERY": 10.0,
    "GALAXY": 10.0,
    "SOLR": 10.0
  },
  "ISA_TAB_DIR": "<%= @isa_tab_dir %>",
  "JAVA_ENTITY_EXPANSION_LIMIT": 1000000,
  "LANGUAGE_CODE": "en-us",
  "LIBS_DIR": "/opt",
  "LOG_LEVEL": "DEBUG",
  "MEDIA_ROOT": "<%= @media_root %>",
  "MEDIA_URL": "/media/",
  "NEO4J": {
    "BASE_URL": "http://localhost:7474",
    "CONSTRAINTS": [
      {
        "label": "Class",
        "property": [
          {
            "name": "name",
            "unique": true
          },
          {
            "name": "uri",
            "unique": true
          }
        ]
      },
      {
        "label": "Ontology",
        "property": [
          {
            "name": "acronym",
            "unique": true
          },
          {
            "name": "uri",
            "unique": true
          }
        ]
      },
      {
        "label": "User",
        "property": [
          {
            "name": "id",
            "unique": true
          }
        ]
      },
      {
        "label": "DataSet",
        "property": [
          {
            "name": "id",
            "unique": true
          }
        ]
      }
    ]
  },
  "REFINERY_BANNER": "",
  "REFINERY_BANNER_ANONYMOUS_ONLY": false,
  "REFINERY_DATA_IMPORT_DIR": "<%= @import_dir %>",
  "REFINERY_EXTERNAL_AUTH": false,
  "REFINERY_EXTERNAL_AUTH_MESSAGE": "",
  "REFINERY_FILE_SOURCE_MAP": {},
  "REFINERY_GALAXY_ANALYSIS_CLEANUP": "on_success",
  "REFINERY_GOOGLE_ANALYTICS_ID": "",
  "REFINERY_INNER_NAVBAR_HEIGHT": 20,
  "REFINERY_AUXILIARY_FILE_GENERATION": "on_file_import",
  "REFINERY_MAIN_LOGO": "",
  "REFINERY_REGISTRATION_CLOSED_MESSAGE": "",
  "REFINERY_REPOSITORY_MODE": false,
  "REFINERY_SOLR_BASE_URL": "http://localhost:8983/solr/",
  "REFINERY_SOLR_SPACE_DYNAMIC_FIELDS": "_",
  "REFINERY_URL_SCHEME": "http",
  "REFINERY_WELCOME_EMAIL_MESSAGE": "Your account has been activated!\nTo log into Refinery, please follow this link and use your username or email address and password provided when you registered:\nhttp://192.168.50.50:8000/accounts/login/\nIf you have any questions, please contact the Refinery team at admin@example.org",
  "REFINERY_WELCOME_EMAIL_SUBJECT": "Welcome to Refinery",
  "REGISTRATION_OPEN": true,
  "SECRET_KEY": "SECRET",
  "SERVER_EMAIL": "<%= @server_email || "root@localhost" %>",
  "SOLR_SYNONYMS": false,
  "SOLR_CUSTOM_SYNONYMS_FILE": "<%= @solr_custom_synonyms_file %>",
  "SOLR_LIB_DIR": "<%= @solr_lib_dir %>",
  "STATIC_ROOT": "",
  "STATIC_URL": "/static/",
  "TIME_ZONE": "America/New_York",
  "TIMEOUT": {
    "CELERY": 2.0,
    "GALAXY": 2.0,
    "SOLR": 2.5
  },
  "TOKEN_DURATION": 3,
  "USE_I18N": true,
  "USE_L10N": true
}
